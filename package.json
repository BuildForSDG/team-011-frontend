{
  "name": "farmlord",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "test:ci": "npm run clean && npm run test -- --watch=false --code-coverage --no-progress --browsers=ChromeHeadlessCI",
    "test:e2e-ci": "npm run e2e -- --protractor-config=e2e/protractor-ci.conf.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:dev": "ng build",
    "clean": "rm -rf dist coverage",
    "build:ci": "npm run clean && npm run lint && npm run test:ci && npm run build",
    "install:clean": "rm -rf node_modules/ && rm -rf dist/ && rm -rf package-lock.json && npm install && npm start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.6",
